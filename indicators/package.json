{
  "name": "position-keeper-indicators",
  "version": "1.0.0",
  "description": "Continuous Solana token data collection service",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "npx tsx src/index.ts",
    "clean": "if exist dist rmdir /s /q dist",
    "test": "npm run build && npm run start",
    "build:check": "tsc --noEmit",
